{
  "name": "doko-kasse",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/styles": "^4.11.1",
    "@salesforce-ux/design-system": "^2.13.7",
    "@salesforce/design-system-react": "^0.10.23",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.20.0",
    "babel-plugin-root-import": "^6.6.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@salesforce/babel-preset-design-system-react": "^3.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "lint": "eslint ./{src,public}/**/*.{js,ts,tsx}",
    "format": "prettier --write \"**/*.{css,html,js,ts,tsx,json,md,yaml,yml}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{css,html,js,ts,tsx,json,yaml,yml,md}": [
      "prettier --write"
    ],
    "**/{src,public}/**/*.{js,ts,tsx}": [
      "eslint"
    ]
  }
}
